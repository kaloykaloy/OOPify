<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/content">
    <com.example.myapplication.CustomVideoView
        android:id="@+id/backgroundVideo"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="150dp"
        android:layout_height="60dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/back_button"
        android:layout_gravity="top|start"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:elevation="10dp" />

    <com.example.myapplication.ZoomLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">

        <FrameLayout
            android:layout_width="350dp"
            android:layout_height="350dp">

            <LinearLayout
                android:id="@+id/map"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/map"
                android:gravity="center">
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level1"
                android:layout_width="70dp"
                android:layout_height="55dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="50dp">

                <ImageButton
                    android:id="@+id/level1Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level2"
                android:layout_width="55dp"
                android:layout_height="50dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginLeft="45dp"
                android:layout_marginBottom="90dp">

                <ImageButton
                    android:id="@+id/level2Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level3"
                android:layout_width="55dp"
                android:layout_height="45dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|start"
                android:layout_marginLeft="70dp"
                android:layout_marginBottom="105dp">

                <ImageButton
                    android:id="@+id/level3Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level4"
                android:layout_width="55dp"
                android:layout_height="45dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="155dp">

                <ImageButton
                    android:id="@+id/level4Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level5"
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|start"
                android:layout_marginLeft="45dp"
                android:layout_marginBottom="170dp">

                <ImageButton
                    android:id="@+id/level5Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level6"
                android:layout_width="40dp"
                android:layout_height="80dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginRight="57dp"
                android:layout_marginBottom="230dp">

                <ImageButton
                    android:id="@+id/level6Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3"
                    android:layout_marginBottom="30dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level7"
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="260dp">

                <ImageButton
                    android:id="@+id/level7Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3"
                    android:layout_marginBottom="30dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level8"
                android:layout_width="40dp"
                android:layout_height="60dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginLeft="62dp"
                android:layout_marginBottom="250dp">

                <ImageButton
                    android:id="@+id/level8Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3"
                    android:layout_marginBottom="10dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level9"
                android:layout_width="60dp"
                android:layout_height="130dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|end"
                android:layout_marginRight="35dp"
                android:layout_marginBottom="120dp">

                <ImageButton
                    android:id="@+id/level9Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3"
                    android:layout_marginBottom="25dp"
                    android:layout_marginRight="5dp"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/level10"
                android:layout_width="40dp"
                android:layout_height="100dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginLeft="15dp"
                android:layout_marginBottom="150dp">

                <ImageButton
                    android:id="@+id/level10Image"
                    android:layout_width="40dp"
                    android:layout_height="20dp"
                    android:background="@null"
                    android:scaleType="fitCenter"
                    android:src="@drawable/star3"
                    android:layout_marginBottom="25dp" />
            </LinearLayout>


        </FrameLayout>
    </com.example.myapplication.ZoomLayout>



</FrameLayout>
